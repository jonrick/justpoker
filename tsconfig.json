{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dist/server",
        "baseUrl": ".",
        "typeRoots":[
          "node_modules/@types"
        ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
      "dist",
      "node_modules"
    ]
}

